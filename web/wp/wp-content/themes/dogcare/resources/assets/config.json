{
    "entry": {
        "main": [
            "./scripts/main.js",
            "./styles/main.scss"
        ],
        "customizer": [
            "./scripts/customizer.js"
        ]
    },
    "publicPath": "/app/themes/dogcare",
    "devUrl": "http://localhost/dogcare/web",
    "proxyUrl": "http://localhost:3000",
    "cacheBusting": "[name]_[hash:8]",
    "headers": { "Access-Control-Allow-Origin": "*" },
    "watch": [
        "app/**/*.php",
        "config/**/*.php",
        "resources/views/**/*.php"
    ]
}
